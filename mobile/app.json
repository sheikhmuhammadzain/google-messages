{
  "expo": {
    "name": "Messages",
    "slug": "google-messages-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "scheme": "googlemessages",
    "jsEngine": "hermes",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#1A73E8"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.googlemessages.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1A73E8"
      },
      "package": "com.googlemessages.app",
      "permissions": [
        "READ_SMS",
        "SEND_SMS",
        "RECEIVE_SMS",
        "READ_CONTACTS",
        "WRITE_SMS",
        "RECEIVE_MMS",
        "CAMERA",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "intentFilters": [
        {
          "action": "android.intent.action.SENDTO",
          "data": {
            "scheme": "smsto"
          }
        },
        {
          "action": "android.intent.action.SENDTO",
          "data": {
            "scheme": "sms"
          }
        }
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Messages to scan QR codes for pairing."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a57df01d-6068-4e7a-9ba6-f18bb59fccf6"
      }
    },
    "owner": "zainsh2"
  }
}
